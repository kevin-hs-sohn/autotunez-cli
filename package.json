{
  "name": "autotunez",
  "version": "0.5.36",
  "description": "CLI for vibe coding with Claude - transform messy prompts into optimized Claude Code instructions",
  "type": "module",
  "bin": {
    "autotunez": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-hs-sohn/autotunez-cli.git"
  },
  "homepage": "https://github.com/kevin-hs-sohn/autotunez-cli#readme",
  "bugs": {
    "url": "https://github.com/kevin-hs-sohn/autotunez-cli/issues"
  },
  "keywords": [
    "claude-code",
    "claude",
    "anthropic",
    "ai",
    "cli",
    "vibe-coding",
    "prompt-engineering",
    "coding-assistant",
    "developer-tools",
    "autonomous-coding",
    "llm",
    "ai-coding",
    "autotunez"
  ],
  "author": "Kevin Sohn",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "vibesafu": "^0.1.12",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "ink": "4.4.1",
    "ink-spinner": "5.0.0",
    "ink-text-input": "5.0.1",
    "ora": "^9.1.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@types/react": "18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^9.39.2",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
